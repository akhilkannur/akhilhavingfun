{
  "rewrites": [
    {
      "source": "/email-checker/(.*)",
      "destination": "/email-deliverability-checker/app.py"
    }
  ],
  "builds": [
    {
      "src": "email-deliverability-checker/app.py",
      "use": "@vercel/python"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=1, stale-while-revalidate"
        }
      ]
    }
  ]
}